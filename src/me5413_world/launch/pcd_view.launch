<?xml version="1.0"?>
<launch>
  <arg name="pcd_file"/>
  <arg name="rviz_config"/>
  <node pkg="pcl_ros" type="pcd_to_pointcloud" name="pcd_to_pointcloud" args="$(arg pcd_file) 5" output="screen">
    <param name="frame_id" value="map" />
  </node>
  <node type="rviz" name="rviz" pkg="rviz" args="-d $(find me5413_world)/rviz/pcd_view.rviz" output="log" respawn="true"/>
</launch>

